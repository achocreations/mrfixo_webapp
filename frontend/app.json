{
    "name": "mrfixo",
    "displayName": "MrFixo",
    "expo": {
      "name": "MrFixo",
      "slug": "mrfixo",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.mrfixo.app",
        "buildNumber": "1.0.0",
        "infoPlist": {
          "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby service providers."
        }
      },
      "android": {
        "package": "com.mrfixo.app",
        "versionCode": 1,
        "permissions": [
          "ACCESS_FINE_LOCATION"
        ],
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      }
    }
  }
  